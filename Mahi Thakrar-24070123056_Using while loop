#include <iostream>
using namespace std;
int main() {
    char name;
    int office_id;
    int pass=2568;
    int user_pass;
    cin>>name;
    cout<<"Welcome"<<name<<endl;
    cout<<"Enter your office id number:"<<endl;
    cin>>office_id;
    cout<<"Enter your password:"<<endl;
    cin>>user_pass;
    while(user_pass>0){
        if(pass != user_pass){
            cout<<"Incorrect password.\n"<<endl;
            cout<<"Forgot Password?\n"<<endl;
            cout<<"Reset Password,click here"<<endl;
        }
    }

    return 0;
}
